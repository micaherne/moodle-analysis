  "description": "Utilities for helping with static analysis of Moodle code",
  "type": "library",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Michael Aherne",
      "email": "micaherne@blueyonder.co.uk",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {

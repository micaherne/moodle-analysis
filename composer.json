{
  "name": "micaherne/moodle-analysis",
  "description": "Utilities for helping with static analysis of Moodle code, and various extracted data.",
  "type": "library",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Michael Aherne",
      "email": "micaherne@blueyonder.co.uk",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "MoodleAnalysis\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MoodleAnalysis\\": "tests/"
    }
  },
  "require": {
    "php": "^8.3",
    "ext-simplexml": "*",
    "composer/semver": "^3.4",
    "psr/log": "^3.0"

  },
  "require-dev": {
    "fidry/cpu-core-counter": "^1.2.0",
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^11.0",
    "react/event-loop": "^v1.5.0",
    "react/child-process": "^0.6.6",
    "react/socket": "^1.16.0",
    "rector/rector": "^1.0",
    "symfony/cache": "^7.0",
    "symfony/process": "^7.0",
    "symfony/filesystem": "^7.0",
    "symfony/finder": "^7.0",
    "symfony/console": "^7.0",
    "nikic/php-parser": "^5.0",
    "react/promise": "^3.2.0",
    "react/async": "^4.3.0"
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse",
    "rector": "vendor/bin/rector process",
    "tests": "vendor/bin/phpunit tests"
  },
  "bin": [
    "bin/moodle-analysis"
  ]
}
